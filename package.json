  "scripts": {
    "build": "yarn install && && yarn css:build && yarn start",
    "css:watch": "(rm -f assets/css/app.css) & sass --watch assets/scss/app.scss:assets/css/app.css",
    "css:build": "sass assets/scss/app.scss assets/css/app.css",
    "babel:build": "babel ./assets/javascripts/index.js -d ./assets/javascripts/babel.js",
    "webpack:watch": "webpack --config webpack.config.js  --mode development",
    "webpack:build": "webpack ./assets/javascripts/index.js -o ./assets/javascripts/main.js --mode production",
    "start": "eslint .eslintrc.js .eslintignore app.js  && node -r dotenv/config app.js dotenv_config_path=./.env",
    "dev": "eslint .eslintrc.js .eslintignore ./app.js && nodemon -r  ./app.js",
    "test": "mocha",
    "lint": "eslint .eslintrc.js .eslintignore ./app/ app.js"
  },
